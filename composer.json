{
    "name": "instaclick/base-riak-bundle",
    "description": "This bundle provides basic (riak) session support for Symfony2 applications",
    "keywords": ["base", "riak", "session"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/instaclick/ObjectCalisthenicsSniffs.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/instaclick/CodingStandard.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-riak": ">=0.5",
        "symfony/symfony": "2.2.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "instaclick/base-test-bundle": "dev-master",
        "squizlabs/php_codesniffer": "dev-master",
        "instaclick/symfony2-coding-standard": "dev-remaster",
        "instaclick/object-calisthenics-sniffs": "dev-master",
        "instaclick/coding-standard": "1.1"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\RiakBundle": "" }
    },
    "target-dir": "IC/Bundle/Base/RiakBundle",
    "minimum-stability": "dev"
}
